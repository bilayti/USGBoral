    // Hide & Show Tab Menu
    var _CurrentActive = 'liMenu1';
    function CurrentActive(currTab) {
        ////debugger;
        $('#divMyReportsPendingOrderReport').hide();
        $('#divMyReportsItemPurchase').hide();
        $('#divMyReportsInvoice').hide();
        $('#divMyReportsCredit').hide();
        $('#divMyReportsBank').hide();
        $('#divMyReportsAccount').hide();
        $('#divReport').hide();
        _CurrentActive = currTab.id;
        var MyMenuLength = $('#MyUl li').length;
        for (var i = 1; i <= MyMenuLength; i++) {
            $('#liMenu' + i + '').removeClass('active')
            $('#liMenu' + i + '').removeClass('CurrentActiveTab')
        }
        $('#' + currTab.id + '').addClass('CurrentActiveTab')
        if (currTab.id == 'liMenu1') {
            $('#divReportsAgeing').show();
            $('#divReports').hide();
            $('#divPenOrder').hide();
            $('#divAccountStatements').hide();
        }
        else if (currTab.id == 'liMenu2') {
            $('#divReports').hide();
            $('#divReportsAgeing').hide();
            $('#divPenOrder').show();
            $('#divAccountStatements').hide();
        }
        else if (currTab.id == 'liMenu5') {
            $('#divReports').hide();
            $('#divReportsAgeing').hide();
            $('#divPenOrder').hide();
            $('#divAccountStatements').show();
        }
        else {
            $('#divReports').show();
            $('#divReportsAgeing').hide();
            $('#divPenOrder').hide();
            $('#divAccountStatements').hide();
        }

        //$('#tblMyReports').empty();
        //$('#theadmydata').empty();
        //$('#tbodymydata').empty();
        $('#txtFromDT').val('');
        $('#txtToDT').val('');
    }

    //Find Customer Name
    function OnChangeFill() {
        var CardCode = $('#ddlCustomerName').val();
        $.ajax({
            url: "/HomePage/CustomerOnChange",
            type: "POST",
            data: JSON.stringify({ 'CardCode': CardCode }),
            dataType: "json",
            traditional: true,
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                if (data.length > 0) {
                    $('#txtAddress').val(data[0].Address)
                    $('#txtAddress2').val(data[0].City)
                    $('#txtCity').val(data[0].City)
                    $('#txtPostCode').val(data[0].ZipCode)
                    $('#txtState').val(data[0].State1)
                    $('#txtCountry').val(data[0].Country)
                    $('#txtCity').val(data[0].Area)
                    $('#txtTerritory').val(data[0].Territory)
                    $('#txtSalesPerson').val(data[0].SlpCode)
                    $('#txtContract').val(data[0].CntctPrsn)
                    $('#txtPhone').val(data[0].Phone1)
                    $('#txtFax').val(data[0].Fax)
                    $('#txtEmail').val(data[0].E_Mail)
                    $('#txtLastPaymentOn').val(data[0].LastPaymentOn)
                    $('#txtPAN').val(data[0].PAN)
                    $('#txtTIN').val(data[0].TIN)
                    $('#txtGSTINNo').val(data[0].GSTINNo)
                    $('#txtBalance').val(data[0].BalanceSys)
                    $('#txtOverdue15Less').val(data[0].OverDueLess15)
                    $('#txtOverdue15Gre').val(data[0].OverDueGre15)
                    $('#txtOverDueDays').val(data[0].OverDueDays)
                    $('#txtExpPayment').val(data[0].ExpPayment)
                    $('#txtShortFall').val(data[0].Shortfall)
                    $('#txtCeiling').val(data[0].ordervalue)
                    $('#txtWarehouse').val(data[0].Warehouse)
                    $('#txtCreditLimit').val(data[0].CreditLimit)
                    $('#txtCreditDate').val(data[0].CreditDate)
                    $('#txtConsumed').val(data[0].Consumed)
                    $('#txtOrderValue').val(data[0].Order_Value)
                    $('#txtLimit').val(data[0].Limit)
                    $('#txtTotalOutstanding').val(data[0].Total_Outstanding)
                    $('#txtAvailableLimit').val(data[0].Available_Limit)
                    $('#txtOverduesdays').val(data[0].MaxOverduedays)
                    $('#txtTotalOverdue').val(data[0].Totaloverdue)
                    $('#txtCerditBalance').val(data[0].Cerditbalance)
                    $('#spnMarqueeTotal').html(data[0].Total_Outstanding)
                    $('#spnMarqueeOverdue').html(data[0].Totaloverdue)


                    Clear();
                }
                else {
                    alert("No Record Found");
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
