    <script type="text/javascript">
        $(document).ready(function () {
            $('#aLogout').click(function () {
                window.location.href = '../LogOutPage.aspx';
            });
            GetUserReportAuthorization();
        });

        // Hide Report Tab According to User Authorization
        function GetUserReportAuthorization() {
            var chkAging = '';
            var chkPending = '';
            var chkItem = '';
            var chkInvoice = '';
            var chkAccount = '';
            var chkCollection = '';
            var chkCredit = '';
            //debugger;
            $.ajax({
                url: "/Sale/GetUserReportAuthorization",
                type: "POST",
                data: JSON.stringify({}),
                dataType: "json",
                traditional: true,
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    if (data.length > 0) {
                        chkAging = data[0].Aging;
                        chkPending = data[0].PendingOrder;
                        chkItem = data[0].ItemPurchase;
                        chkInvoice = data[0].Invoice;
                        chkAccount = data[0].AccountStatement;
                        chkCollection = data[0].CollectionSummary;
                        chkCredit = data[0].CreditNote;
                        if (chkAging == "N") {
                            $('#liMenu1').hide();
                        }
                        if (chkPending == "N") {
                            $('#liMenu2').hide();
                        }
                        if (chkItem == "N") {
                            $('#liMenu3').hide();
                        }
                        if (chkInvoice == "N") {
                            $('#liMenu4').hide();
                        }
                        if (chkAccount == "N") {
                            $('#liMenu5').hide();
                        }
                        if (chkCollection == "N") {
                            $('#liMenu6').hide();
                        }
                        if (chkCredit == "N") {
                            $('#liMenu7').hide();
                        }
                    }
                    else {
                        //alert("No Record Found");
                    }
                },
                error: function () {
                    alert("Database connection has lost, please check your internet connection and relogin!!!");
                }
            });
        }

        // Hide & Show Tab Menu
        var _CurrentActive = 'liMenu1';
        function CurrentActive(currTab) {
            $('#divMyReportsPendingOrderReport').hide();
            $('#divMyReportsItemPurchase').hide();
            $('#divMyReportsAccount').hide();
            $('#divMyReportsInvoice').hide();
            $('#divMyReportsCredit').hide();
            $('#divMyReportsBank').hide();
            $('#divMyReportsAccount').hide();
            $('#divReport').hide();
            _CurrentActive = currTab.id;
            var MyMenuLength = $('#MyUl li').length;
            for (var i = 1; i <= MyMenuLength; i++) {
                $('#liMenu' + i + '').removeClass('active')
                $('#liMenu' + i + '').removeClass('CurrentActiveTab')
            }
            $('#' + currTab.id + '').addClass('CurrentActiveTab')
            if (currTab.id == 'liMenu1') {
                $('#divReportsAgeing').show();
                $('#divReports').hide();
                $('#divPenOrder').hide();
                $('#divAccountStatements').hide();
            }
            else if (currTab.id == 'liMenu2') {
